@model IEnumerable<AIRMAM5.DBEntity.Models.Booking.MyBookingModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_BlankLayout.cshtml";
}
@section Styles{
    <link href="~/Content/iftame_style/MyBooking/MyBookingIndex.min.css" rel="stylesheet" />
}

@HtmlHelper.Breadcrumb(new string[] { "檔案調用", "我的調用狀態" })
<div class="_Filter">
    <div class="ui stackable three column grid">
        <div class="computer only column nopadding">
            <div class="ui horizontal inverted divider">
                <span class="ui circular grey label">1</span>
                加入借調
            </div>
        </div>
        <div class="computer only column  nopadding">
            <div class="ui horizontal inverted divider">
                <span class="ui circular grey label">2</span>
                我的調用清單
            </div>
        </div>
        <div class="column  nopadding">
            <div class="ui horizontal inverted divider">
                <span class="ui circular red  label">3</span>
                我的調用狀態
            </div>
        </div>
    </div>
    @Html.Action("_Search", "MyBooking")
</div>
<div id="Table"></div>

<!--燈箱區域-->
@Html.Partial("~/Views/Shared/_ModalArea.cshtml")

@section Scripts{
    <script src="~/Content/dayJS/dayjs.min.js"></script>
    <script src="~/Content/semanticUIdatepicker/calendar.min.js" type="text/javascript"></script>
    @Html.Partial("~/Views/Bundle/MyBookingIndex_Js.cshtml")
    @*<script src="~/Scripts/dist/MyBookingIndex.min.js"></script>*@
}
