@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_BlankLayout.cshtml";
}
@section Styles{
    <link href="~/Content/iftame_style/Booking/BookingIndex.min.css" rel="stylesheet" />
}
@HtmlHelper.Breadcrumb(new string[] { "檔案調用", "管理調用狀態" })
<div class="_Filter">
    @Html.Action("_Search", "Booking")
</div>
<div id="Table"></div>

<!--燈箱區域-->
@Html.Partial("~/Views/Shared/_ModalArea.cshtml")
@HtmlHelper.ConfirmModal("RebookingConfirm", "重新借調確認", "確定要重新借調選定的檔案?")
@HtmlHelper.ConfirmModal("CancelbookingConfirm", "取消借調確認", "確定要取消借調選定的檔案?")

@section Scripts{
    <script src="~/Content/dayJS/dayjs.min.js"></script>
    <script src="~/Content/semanticUIdatepicker/calendar.min.js" type="text/javascript"></script>
    @Html.Partial("~/Views/Bundle/BookingIndex_Js.cshtml")
    @*<script src="~/Scripts/dist/BookingIndex.min.js"></script>*@
}

