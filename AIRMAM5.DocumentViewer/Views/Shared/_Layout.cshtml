<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="description" content="Asp.net mvc document viewer free sample code in C# and vb.net. View office documents and pdf files online in asp.net" />

    <!-- Drop zone css (for upload)-->
    <link href="/scripts/dropzone/basic.css" rel="stylesheet" />
    <link href="/scripts/dropzone/dropzone.css" rel="stylesheet" />

    <!-- Thickbox (For print) -->
    <link href="~/Content/thickbox.css" rel="stylesheet" />
    <!-- Bootstrap 4 -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">

    @RenderSection("css", required: false)
    @RenderSection("styles", required: false)
</head>
<body>
    @RenderBody()

    <!-- jQuery-->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js" type="text/javascript"></script>

    <!-- Bootstrap 4 JS -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>

    <!-- Drop Zone JS (For upload) -->
    <script type="text/javascript" src="~/scripts/dropzone/dropzone.js"></script>

    <!-- Thickbox (For print) -->
    <script src="~/Content/thickbox-compressed.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
